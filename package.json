{
  "name": "justice-plurielle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bash:production": "scalingo -a justice-plurielle run bash",
    "build": "next build",
    "deadcode": "ts-prune --ignore 'pages'",
    "depcheck": "depcheck",
    "dev": "yarn start:db && next dev",
    "lighthouse": "lhci autorun",
    "lint:css": "stylelint '(components|configuration)/**/*.css'",
    "lint:typescript": "next lint --max-warnings=0",
    "maj_lieux": "ts-node -T backend/infrastructure/controllers/majDesLieuxCron.ts",
    "migration:up": "npx typeorm-ts-node-commonjs migration:run -d ./database/dataSource.ts",
    "migration:down": "npx typeorm-ts-node-commonjs migration:revert -d ./database/dataSource.ts",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create",
    "psql:local": "docker-compose up -d postgres && docker exec -it justice_plurielle_db psql -U justice_plurielle justice_plurielle",
    "psql:production": "scalingo -a justice-plurielle pgsql-console",
    "psql:test": "docker-compose up -d postgres && docker exec -it justice_plurielle_db_test psql -U justice_plurielle justice_plurielle",
    "start": "next start",
    "start:db": "docker-compose down && docker-compose up -d && bash -c \"while ! docker-compose logs | grep -q 'listening on IPv4 address'; do echo 'Waiting for database'; sleep 0.5; done\"; yarn migration:up",
    "test": "NODE_ENV=test yarn migration:up && vitest --run",
    "test:coverage": "yarn test --coverage",
    "test:mutation:frontend": "npx stryker run stryker-frontend.conf.json",
    "test:mutation:backend": "npx stryker run stryker-backend.conf.json",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.21.1",
    "accessible-autocomplete": "^2.0.4",
    "dotenv-defaults": "^5.0.2",
    "googleapis": "^109.0.1",
    "leaflet": "^1.9.3",
    "next": "13.1.5",
    "next-pwa": "^5.6.0",
    "pg": "^8.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.2.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@lhci/cli": "^0.10.0",
    "@stryker-mutator/core": "^6.3.1",
    "@stryker-mutator/jest-runner": "^6.3.1",
    "@stryker-mutator/typescript-checker": "^6.3.1",
    "@swc/core": "^1.3.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/dotenv-defaults": "^2.0.1",
    "@types/leaflet": "^1.9.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.28.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.0",
    "next-router-mock": "^0.8.0",
    "stylelint": "^14.15.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "typescript": "^4.9.3",
    "vitest": "^0.28.1"
  },
  "engines": {
    "node": "18.12.1"
  },
  "packageManager": "yarn@3.3.0"
}
